{
    "compilerOptions": {
        "outDir": "dist",
        "sourceMap": false,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "esnext",
        "lib": ["ESNext"],
        "skipLibCheck": true,
        "strict": true,
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true,
        "declaration": false
    }
}